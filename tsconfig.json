{
  "compilerOptions": {
    "lib": [
      "esnext"
    ],
    "module": "commonjs",
    "target": "es2020",
    "moduleResolution": "node",
    "strict": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true,
    "allowJs": true
  }
}